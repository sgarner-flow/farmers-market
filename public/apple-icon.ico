      },
      freeMode: false,
      cssMode:isMac,
      direction: "vertical",
      on:{
        activeIndexChange:activeIndexChange
      }
    });
    obj.on('slideChange', function () {        
      try{
        gtag('event', 'carousel_scroll', {
                    'carousel_name': $(this.el).data('carousel_name'),
