var recapthaObjs=[];
var recaptchLoaded=false;

function recaptchaOnload(){        
  recaptchLoaded=true;
  for(var i=0;i<recapthaObjs.length;i++){
    try{
      recapthaObjs[i].captacha= grecaptcha.render(
          recapthaObjs[i].data.recaptcha.id,
          {'size':((recapthaObjs[i].data.recaptcha?.size)?recapthaObjs[i].data.recaptcha.size:'normal'),
          'callback':recapthaObjs[i].f_isFormValid.bind(recapthaObjs[i]),
          "sitekey": '6LcFkWUhAAAAALWUQ70PEhcGWEgTxIIw4imsHQmp', 
          "theme": "light"}
        );

    }
    catch(e){console.log(e)}
  }
  console.log('recaptcha loaded');
}
var _settings={propertyKey:'flow', 
  propertyId:'00000000-0000-0000-0000-000000000000', furnishedAmenity: 'furnished', 
  furnishedPriceMatrix: '',
  priceGrouping: '1'
}
try{
  var _mainReferrer = null;
  if(sessionStorage.getItem('mainReferrer')){
    _mainReferrer = JSON.parse(sessionStorage.getItem('mainReferrer'));
  }
  // Check if mainReferrer hasn't been set yet
  if (!_mainReferrer) {
    var referrer=null;
    // Store the http referrer in the web storage  
    if(document.referrer?.length>0 && document.referrer.indexOf(window.location.host)<0){
      referrer= document.referrer;
    }
    // Store utm_source and utm_medium
    var params = new URLSearchParams(window.location.search)
    var utm_source=params.get('utm_source');
    var utm_medium=params.get('utm_medium');
    var utm_campaign=params.get('utm_campaign');
    if((utm_source && utm_medium)||referrer){
      _mainReferrer={referrer:referrer, utm_source:utm_source, utm_medium:utm_medium, utm_campaign:utm_campaign};
      sessionStorage.setItem('mainReferrer', JSON.stringify(_mainReferrer));
    }
  }
}
catch(ex){
  console.log(ex);
}

function _getSourceName(){  
  try{
    var source=_getSource();
    return (source)?source.sourceName:'Property Website';
  }
  catch(e){console.log(e);return 'Property Website';}
}

function _getSourceQuery(){  
  try{
    return ((_mainReferrer && _mainReferrer.utm_source)?'utm_source='+_mainReferrer.utm_source:'')+
    ((_mainReferrer && _mainReferrer.utm_medium)?'&utm_medium='+_mainReferrer.utm_medium:'')+
    ((_mainReferrer && _mainReferrer.utm_campaign)?'&utm_campaign='+_mainReferrer.utm_campaign:'');
  }
  catch(e){console.log(e);return 'Property Website';}
}

function _getSource(){  
  var source=null;
  try{
      if(window._mainReferrer && window.propertyObj?.marketing?.sources?.length>0){
          var sources = propertyObj.marketing.sources;
          sources.forEach((s) => {
              if(s.referrals){
                  s.referrals.forEach((r) => { 
                      if(r.referralUrlHost?.length>0 
                          && _mainReferrer.referrer?.toLowerCase().indexOf(r.referralUrlHost.toLowerCase())>-1){
                          source=s;
                      }
                      
                      if(r.referralUrlQuery?.length>0 
                          && r.referralUrlQuery.toLowerCase().indexOf('utm_source='+_mainReferrer.utm_source?.toLowerCase())>-1
                          && r.referralUrlQuery.toLowerCase().indexOf('utm_medium='+_mainReferrer.utm_medium?.toLowerCase())>-1){
                          source=s;
                          source['query'] = r.referralUrlQuery.indexOf('&utm_campaign')>-1?r.referralUrlQuery.substring(0, r.referralUrlQuery.indexOf('&utm_campaign')):r.referralUrlQuery;
                          if(r.referralUrlQuery.toLowerCase().indexOf('utm_campaign='+_mainReferrer.utm_campaign?.toLowerCase())>-1){
                              source['query'] = r.referralUrlQuery;
                              return source;
                          }
                      }
                  });
              }
          });
      }
  }
  catch(e){console.log(e)}
  return source;
}

_contactUsCls = function (data) {
  this.data = data;
  this.data.values = {};
  this.captacha=null;

  this.init=function(){
    try{          
      //set captcha
      if(this.data?.recaptcha?.id && !(this.data?.recaptcha?.disable)){
        if(!recaptchLoaded)
          recapthaObjs.push(this)
        else{
          this.captacha= grecaptcha.render(
            data.recaptcha.id,
            {callback:this.f_isFormValid.bind(this), "sitekey": '6LcFkWUhAAAAALWUQ70PEhcGWEgTxIIw4imsHQmp', "theme": "light"}
          );
        }
      }
      //set onkeyup
      if(typeof this.data.keyupSelector === 'string')
        this.data.keyupSelector=$(this.data.keyupSelector);

      this.data.keyupSelector.on('keyup keypress blur change', () => {
        this.f_isFormValid();
      });
      
      //set submit
      if(typeof this.data.submit.selector === 'string')
        this.data.submit.selector=$(this.data.submit.selector);
      
      this.data.submit.selector.on('click', (event ) =>{
        event.stopPropagation();
        this.f_submitForm((err, result) => {
          if(this.data.submit.callback)
            this.data.submit.callback(err, result)
        });
        return false;
      });
    }
    catch(e){console.log(e)}
  }

  this.getValues = function(){
    for(i in this.data.form){
      try{              
        if(typeof this.data.form[i] === 'string')
          this.data.form[i] = $('#'+this.data.form[i]);
        
        this.data.values[i]=this.data.form[i].val();
      }
      catch(e){console.log(e)}
    }
  }
  
  this.f_isFormValid = function(response) {
    this.getValues();
    var values=this.data.values;
    var recaptcha_result='';      
    var result = '';  
    if(!(!(Object.hasOwn(values, 'phone'))||
          values.phone==''||
          (
            !!values.phone
            && String(values.phone).toLowerCase().match(
              /^([0-9\-\(\)\+\s]{5,})$/)?.length>0
          )
        ))
        result='Please enter a valid phone number';
    if(!((values.phone=='' && !this.data.form.phone.data('areacode')) ||
      (this.data.form.phone && !this.data.form.phone.data('areacode')) ||
      (this.data.form.phone && this.data.form.phone.data('areacode')
            && !!values.phone
            && String(values.phone).toLowerCase().match(
              /^\+([0-9\-\(\)\+\s]{8,})$/)?.length>0)))
        result='Please enter phone number with area code ex. +966-11-520-7100';

    if(!(!!values.email && String(values.email).toLowerCase().match(
          /^(([^<>()[\]\\.,;:\s@"]+(\.[^<>()[\]\\.,;:\s@"]+)*)|.(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/
        )?.length>0))
      result='Please enter email address';
    if (!(!(Object.hasOwn(values, 'first')) || !!values.first))result='Please enter first name';
    if (!(!(Object.hasOwn(values, 'last')) || !!values.last))result='Please enter last name';
    if (!(!(Object.hasOwn(values, 'name')) || !!values.name))result='Please enter name';
    if (!(!(Object.hasOwn(values, 'interest')) || !!values.interest))result='Please select interest';
    
    //validate extra fields (check if field is required)
    if(result=='' && Object.hasOwn(this.data, 'extraFields')) {
      try{
        this.data.extraFields.forEach((o)=>{
          if(o.required == 'true' && 
            (($('#'+this.data.preFix+o.name).attr('type')=='checkbox' && !$('#'+this.data.preFix+o.name).is(':checked'))
              || 
              ($('#'+this.data.preFix+o.name).attr('type')!='checkbox' && $('#'+this.data.preFix+o.name).val().length<1))
          ){
            result='missing '+o.title;
          }
        })
      }
      catch(ex){}
    }
    if(result==='' && this.captacha!=null){
        try{
          recaptcha_result = grecaptcha.getResponse(this.captacha);
          this.data.recaptcha.token=recaptcha_result;
          if(!recaptcha_result) result='Missing Captcha';
        }
        catch(e){
          console.log(e);
        }
    }       
    if(result==='') {
      this.data.submit.selector.removeClass('disabled');
    }
    else {
      this.data.submit.selector.addClass('disabled');
    }
    
    if(this.data.submit.errorMsg)
      $(this.data.submit.errorMsg).html(result);
    return result;
  }

  this.f_submitForm = function(f_callback){ 
    const validResult = this.f_isFormValid();
    if(validResult!='') {
      if(f_callback)f_callback(validResult);
    }
    else{
      console.log('valid form');  
      if(!this.data.recaptcha || this.data.recaptcha.token || this.data.recaptcha.disable){
          if(Object.hasOwn(this.data.submit, 'preSubmit'))this.data.submit.preSubmit(this);
          this.data.submit.selector.addClass('disabled');
          var reason = (Object.hasOwn(this.data.values, 'interest'))?this.data.values.interest:'';
          var propertyId = _settings.propertyId;

          if(Object.hasOwn(this.data.values, 'name')){
            const nameParts = this.data.values.name.split(' ');
            this.data.values.first = nameParts[0];
            this.data.values.last = ((nameParts.length<2)?'n/a':nameParts[1]);
          }
          var source=_getSourceName();
          var sourceQuery=_getSourceQuery();
          if(!reason || reason.startsWith('prp_')){
            if(reason && reason.startsWith('prp_')) propertyId=reason.replace('prp_', '');
            if(!Object.hasOwn(this.data, 'extra')) this.data.extra={};
            if(Object.hasOwn(this.data, 'extraFields')) {
              try{
                this.data.extraFields.forEach((o)=>{
                  if($('#'+this.data.preFix+o.name).length>0){
                    this.data.extra[o.name] = $('#'+this.data.preFix+o.name).val();
                  }
                })
              }
              catch(ex){}
            }
            var body = { ...{
                firstName: this.data.values.first,
                lastName: (Object.hasOwn(this.data.values, 'last'))?this.data.values.last:'',
                email: this.data.values.email,
                phone: this.data.values.phone,
                message: this.data.values.message,
                source:source,
                sourceQuery: sourceQuery,
                pageKey: 'pagenotfound',
                url: window.location.href,
                token:this.data.recaptcha?.token,
                extra:this.data.extra
              }, ...this.data.extra};
            fetch('/apipublic/prospect?propertyId='+propertyId, {
              headers: {
                'api-key': 'public',
                'Content-type': 'application/json'
              },
              method: 'POST',
              body: JSON.stringify(body),
            }).then(response => {
              try{
                gtag('event', Object.hasOwn(this.data.submit, 'gtag')?this.data.submit.gtag:'contactus_submit', {
                  'button_name': 'myButton',
                  'screen_name': 'pagenotfound',
                  "email": this.data.values.email,
                  "phone_number": this.data.values.phone
                });
                gtag('set', 'user_data', { "email": this.data.values.email,
                "phone_number": this.data.values.phone });
              }
              catch(ex){console.log(ex)}
              if(f_callback)f_callback(null, this.data);
            });
          }
          else{
            fetch('/apipublic/corporate/contact-us-by-reason', {
              headers: {
                'api-key': 'public',
                'Content-type': 'application/json'
              },
              method: 'POST',
              body: JSON.stringify({
                name: this.data.values.name,
                email: this.data.values.email,
                phone: this.data.values.phone,
                message: this.data.values.message,
                company: this.data.values.company,
                reason: reason,
                source: source,
                sourceQuery: sourceQuery,
                pageKey: 'pagenotfound',
                url: window.location.href,
                token:this.data.recaptcha.token
              }),
              
            }).then(response => {
              console.log(response);
              try{
                gtag('event', 'corporate_contactus_submit', {
                  'button_name': 'myButton',
                  'screen_name': 'pagenotfound',
                  "email": this.data.values.email,
                  "phone_number": this.data.values.phone
                });
                gtag('set', 'user_data', { "email": this.data.values.email,
                "phone_number": this.data.values.phone });
              }
              catch(ex){console.log(ex)}
              if(f_callback)f_callback(null, data);
            });
          }              
      }
    }
  }
  
  this.init();
}

_swiperCls = function () {
  this.f_verticalSwiper = function(element, activeIndexChange){
    var isMac = (navigator.userAgent.toLowerCase().search("macintosh") > -1 && navigator.userAgent.search("iPhone")<0)//(navigator.userAgent.search("Safari") >= 0 || navigator.userAgent.search("Macintosh") >= 0);
