                    "index": this.activeIndex,
                    "page": 'pagenotfound',
                    'carousel_orientation' : "vertical"
                  });
      }
      catch(ex){console.log(ex)}
    });
    return obj;
  } 
  this.f_buildThreeSlideSwiper = function(parentElement){  
    const swiper1 = new Swiper(parentElement+ ' .swiper', {
      loop: true,
