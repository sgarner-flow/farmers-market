    });
    
    if($(parentElement + ' .swiper-wrapper').children().length==3){
      $(parentElement + ' .swiper-button-next').addClass('show-mobile');
      $(parentElement + ' .swiper-button-prev').addClass('show-mobile');
    }
  }

  this.f_buildHorizontalSwiper = function(parentElement, options){  
    const _options={...{
      loop: true,
      pagination: {
        el: parentElement+ ' .swiper-pagination',
        type: 'bullets',
      },            
      breakpoints: {
          0: {
            slidesPerView: 1
          },
          1025: {
